<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen 
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <PreferenceCategory 
        android:title="@string/printer_setting">
        	<EditTextPreference 
        	    android:title="@string/printer_ip" 
        	    android:key="printer_ip" 
        	    android:selectAllOnFocus="true"
        	    android:defaultValue="@string/printer_ip_summary"/>
        	<EditTextPreference 
        	    android:title="@string/printer_port" 
        	    android:key="printer_port" 
        	    android:selectAllOnFocus="true"
        	    android:defaultValue="@string/printer_port_summary"/>
    </PreferenceCategory>
    

</PreferenceScreen>
